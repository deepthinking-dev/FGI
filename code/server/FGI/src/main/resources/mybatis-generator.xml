<?xml version="1.0" encoding="UTF-8"?>  
<!DOCTYPE generatorConfiguration  
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"  
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>

	<context id="DB2Tables" targetRuntime="MyBatis3" defaultModelType="flat">
		<plugin type="org.mybatis.generator.plugins.RenameExampleClassPlugin">
			<property name="searchString" value="[e|E]xample$" />
			<property name="replaceString" value="Criteria" />
		</plugin>

	<commentGenerator>
		<property name="suppressDate" value="true" />
	</commentGenerator>

	<jdbcConnection driverClass="com.mysql.jdbc.Driver"
		connectionURL="jdbc:mysql://127.0.0.1:3306/fgi" userId="root"
		password="123456">
	</jdbcConnection>

	<javaTypeResolver>
		<property name="forceBigDecimals" value="false" />
	</javaTypeResolver>

	<!-- generate Model -->
	<javaModelGenerator targetPackage="deepthinking.fgi.domain"
		targetProject="src/main/java">
		<property name="enableSubPackages" value="true" />
		<property name="trimStrings" value="true" />
	</javaModelGenerator>


	<!-- 自动生成mapper接口, ANNOTATEDMAPPER(注解) -->
	<javaClientGenerator type="ANNOTATEDMAPPER"
		targetPackage="deepthinking.fgi.dao.mapper" implementationPackage="deepthinking.fgi.dao.mapper.impl"
		targetProject="src/main/java">
		<property name="enableSubPackages" value="true" />
	</javaClientGenerator>
		<!--<table tableName="Table_Module" >
               <generatedKey column="ID" sqlStatement="MySql" identity="true"/>
           </table>
		<table tableName="Table_Algorithm" >
               <generatedKey column="ID" sqlStatement="MySql" identity="true"/>
           </table>
		<table tableName="Table_Role" >
               <generatedKey column="ID" sqlStatement="MySql" identity="true"/>
           </table>
		<table tableName="Table_AlgorithmCondition" >
               <generatedKey column="ID" sqlStatement="MySql" identity="true"/>
           </table>
		<table tableName="Table_InterfaceParameters" >
               <generatedKey column="ID" sqlStatement="MySql" identity="true"/>
           </table>-->
		<!--<table tableName="Table_InterfaceRole" >
               <generatedKey column="ID" sqlStatement="MySql" identity="true"/>
           </table>-->
		<!--<table tableName="Table_OperatorInterface" >
               <generatedKey column="ID" sqlStatement="MySql" identity="true"/>
           </table>-->
	</context>
</generatorConfiguration>  
